Cell[BoxData[
 RowBox[{"Manipulate", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"user", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"InputString", "[", "]"}], " ", "tweets"}], "=", 
      RowBox[{"twitter", "[", 
       RowBox[{"\<\"UserReplies\"\>", ",", 
        RowBox[{"\<\"Username\"\>", "\[Rule]", "user"}]}], "]"}]}]}], ";", 
    RowBox[{"tweetText", "=", 
     RowBox[{"Values", "[", 
      RowBox[{"tweets", "\[LeftDoubleBracket]", 
       RowBox[{"All", ",", 
        RowBox[{"{", "\<\"Text\"\>", "}"}]}], "\[RightDoubleBracket]"}], 
      "]"}]}], ";", 
    RowBox[{"tweetSentament", "=", 
     RowBox[{"Classify", "[", "\<\"Sentiment\"\>", "]"}]}], ";", 
    RowBox[{"sentament", "=", 
     RowBox[{"tweetSentament", "/@", "tweetText"}]}], ";", 
    RowBox[{"values", "=", 
     RowBox[{
      RowBox[{
       RowBox[{"Counts", "[", "sentament", "]"}], " ", "moodValue"}], "=", 
      RowBox[{
       FractionBox["1", 
        RowBox[{
         RowBox[{
         "values", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
         "+", 
         RowBox[{
         "values", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}]], 
       RowBox[{
        RowBox[{
        "values", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], " ", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"moodValue", ">", "0"}], ",", 
          InterpretationBox[Cell[BoxData[
            FormBox["\<\"This User Has Been Pretty Happy\"\>", TextForm]], 
            "InlineText",ExpressionUUID->
            "f89d7a61-b2ed-47d4-877a-1dc84afe5d0c"],
           Text["This User Has Been Pretty Happy"]]}], "]"}], " ", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"moodValue", "<", "0"}], ",", 
          InterpretationBox[Cell[BoxData[
            FormBox["\<\"This User Might need some support\"\>", TextForm]], 
            "InlineText",ExpressionUUID->
            "4c8e3995-2bd1-43f3-a5d8-886387c5a0e9"],
           Text["This User Might need some support"]]}], "]"}], " ", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"moodValue", "<", "0"}], ",", 
          RowBox[{"twitter", "[", 
           RowBox[{"\<\"Tweet\"\>", ",", 
            RowBox[{"\<\"Message\"\>", "\[Rule]", 
             
             InterpretationBox["\<\"Hope all is well \
@\\!\\(\\*RowBox[{\\\"user\\\"}]\\)\"\>",
              StringForm["Hope all is well @``", $CellContext`user],
              Editable->False]}]}], "]"}]}], "]"}]}]}]}]}]}], ",", "user"}], 
  "]"}]], "Output",
